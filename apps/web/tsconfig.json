{
  "extends": "@toolings/typescript-config/nextjs.json",
  "compilerOptions": {
    "strict": true,
    "plugins": [
      {
        "name": "next"
      },
      {
        "name": "@effect/language-service"
      }
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "next.config.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
